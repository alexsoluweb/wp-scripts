{
  "name": "asw/<project_name>",
  "description": "Wordpress project",
  "version": "1.0.1",
  "type": "project",
  "license": "MIT",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "minimum-stability" : "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "require-dev": {
    "wpackagist-plugin/loco-translate": "*",
    "wpackagist-plugin/emails-catch-all": "*",
    "wpackagist-plugin/woo-order-test": "*",
    "wpackagist-plugin/query-monitor": "*",
    "wpackagist-plugin/user-switching": "*",
    "wpackagist-plugin/wp-crontrol":"*",
    "wpackagist-plugin/easy-populate-posts":"*"
  },
  "require": {
    "wpackagist-plugin/all-in-one-wp-security-and-firewall": "*"
  }
}